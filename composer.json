{
    "name": "compwright/oauth2-servicefusion",
    "description": "Housecall Pro OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com",
            "homepage": "https://compwright.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "service",
        "fusion",
        "provider"
    ],
    "bin": [
        "bin/servicefusion-client-credentials"
    ],
    "require": {
        "php": "^7.4 || ^8",
        "league/oauth2-client": "^2.5",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Compwright\\OAuth2_Servicefusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\OAuth2_Servicefusion\\": "test/"
        }
    }
}
